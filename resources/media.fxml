<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<BorderPane prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.youz.media.Controller">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="文件">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#openFile" text="选择文件" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="帮助">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#showDonation" text="捐赠" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <ProgressBar fx:id="progressBar" prefHeight="18.0" prefWidth="1393.0" progress="0.0" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <SplitPane prefHeight="657.0" prefWidth="450.0" BorderPane.alignment="CENTER">
         <items>
            <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="655.0" prefWidth="619.0">
              <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <TextArea fx:id="mediaInfoTextArea" prefHeight="325.0" prefWidth="448.0" />
                     </children>
                  </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                     <children>
                        <Pane prefHeight="326.0" prefWidth="447.0">
                           <children>
                               <Text layoutX="22.0" layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" text="片头截取(秒)" />
                               <Text layoutX="22.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="片尾截取(秒)" />
                               <Text layoutX="22.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="视频裁剪频率(秒)" />
                               <TextField layoutX="125.0" layoutY="10.0" prefHeight="23.0" prefWidth="63.0" fx:id="startInterceptVal"/>
                               <TextField layoutX="125.0" layoutY="43.0" prefHeight="23.0" prefWidth="63.0" fx:id="endInterceptVal"/>
                               <TextField layoutX="125.0" layoutY="74.0" prefHeight="23.0" prefWidth="63.0" fx:id="interceptRateVal"/>
                               <Button layoutX="-2.0" layoutY="269.0" mnemonicParsing="false" onAction="#start" prefHeight="57.0" prefWidth="451.0" text="开始" />
                               <CheckBox layoutX="253.0" layoutY="14.0" mnemonicParsing="false" text="导出编单Excel" fx:id="switchExportExcel"/>
                               <Text layoutX="253.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="截图" />
                               <TextField layoutX="290.0" layoutY="43.0" prefHeight="23.0" prefWidth="50.0" />
                               <Text layoutX="351.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="X" />
                               <TextField layoutX="370.0" layoutY="43.0" prefHeight="23.0" prefWidth="50.0" />
                           </children>
                        </Pane>
                     </children>
                  </AnchorPane>
              </items>
            </SplitPane>
         </items>
      </SplitPane>
   </right>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="mediTable" prefHeight="578.0" prefWidth="750.0">
              <columns>
                  <TableColumn fx:id="id" prefWidth="50.0" text="序号" />
                  <TableColumn fx:id="fileName" prefWidth="272.0" text="名称" />
                  <TableColumn fx:id="durationFormat" prefWidth="110.0" text="时长" />
                  <TableColumn fx:id="fileSize" minWidth="0.0" prefWidth="110.0" text="大小" />
                  <TableColumn fx:id="schedule" prefWidth="71.0" text="进度" />
                  <TableColumn prefWidth="120.0" text="操作" />
              </columns>
            </TableView>
            <TextField fx:id="sourcePath" layoutX="84.0" layoutY="588.0" prefHeight="23.0" prefWidth="583.0" />
            <TextField fx:id="targetPath" layoutX="84.0" layoutY="620.0" prefHeight="23.0" prefWidth="583.0" />
            <Text layoutX="14.0" layoutY="604.0" strokeType="OUTSIDE" strokeWidth="0.0" text="原视频路径:" />
            <Text layoutX="14.0" layoutY="636.0" strokeType="OUTSIDE" strokeWidth="0.0" text="   保存路径:" />
            <Button layoutX="681.0" layoutY="588.0" mnemonicParsing="false" onAction="#chooseSourcePath" text="选择" />
            <Button layoutX="681.0" layoutY="620.0" mnemonicParsing="false" onAction="#chooseTargetPath" text="选择" />
         </children>
      </Pane>
   </center>
</BorderPane>
